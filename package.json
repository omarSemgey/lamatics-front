    "dompurify": "^3.2.4",
    "katex": "^0.16.21",
    "path": "^0.12.7",
    "react": "19.0.0",
    "react-axios": "^2.0.6",
    "react-dom": "19.0.0",
    "react-error-boundary": "^5.0.0",
    "vite": "^5.3.10"
